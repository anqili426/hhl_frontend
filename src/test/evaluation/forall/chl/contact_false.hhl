// P1
method compare1(firstName1: Int, lastName1: Int, emails1: Int, firstName2: Int, lastName2: Int, emails2: Int, i: Int) returns (res: Int)
requires forall <_s1>, <_s2> :: _s1[i] == 1 && _s2[i] == 2 ==>
    _s1[firstName1] == _s2[firstName2] && _s1[lastName1] == _s2[lastName2] &&
    _s1[emails1] == _s2[emails2] && _s1[firstName2] == _s2[firstName1] &&
    _s1[lastName2] == _s2[lastName1] && _s1[emails2] == _s2[emails1]
ensures forall <_s1>, <_s2> :: _s1[i] == 1 && _s2[i] == 2 ==>
    ((_s1[res] > 0 && _s2[res] < 0) || (_s1[res] < 0 && _s2[res] > 0) ||
        (_s1[res] == 0 && _s2[res] == 0))
{
    if (firstName1 != 0 && firstName2 != 0) {
        var compFirstName: Int
        if (firstName1 > firstName2) {
            compFirstName := 1
        } else {
            if (firstName1 == firstName2) {
                compFirstName := 0
            } else {
                compFirstName := -1
            }
        }

        if (compFirstName == 0) {
            if (lastName1 != 0 && lastName2 != 0) {
                var compLastName: Int
                if (lastName1 > lastName2) {
                    compLastName := 1
                } else {
                    if (lastName1 == lastName2) {
                        compLastName := 0
                    } else {
                        compLastName := -1
                    }
                }

                if (compLastName == 0) {
                    if (emails1 != 0 && emails2 != 0) {
                        var compEmail: Int
                        if (emails1 > emails2) {
                            compEmail := 1
                        } else {
                            if (emails1 == emails2) {
                                compEmail := 0
                            } else {
                                compEmail := -1
                            }
                        }
                        res := compEmail
                    } else {
                        res := 0
                    }
                } else {
                    res := compLastName
                }
            } else {
                if (emails1 != 0 && emails2 != 0) {
                    var compEmail1: Int
                    if (emails1 > emails2) {
                        compEmail1 := 1
                    } else {
                        if (emails1 == emails2) {
                            compEmail1 := 0
                        } else {
                            compEmail1 := -1
                        }
                    }
                    res := compEmail1
                } else {
                    res := 0
                }
            }
        } else {
            res := compFirstName
        }  
    } else {
        if (lastName1 != 0 && lastName2 != 0) {
                var compLastName1: Int
                if (lastName1 > lastName2) {
                    compLastName1 := 1
                } else {
                    if (lastName1 == lastName2) {
                        compLastName1 := 0
                    } else {
                        compLastName1 := -1
                    }
                }

                if (compLastName1 == 0) {
                    if (emails1 != 0 && emails2 != 0) {
                        var compEmail2: Int
                        if (emails1 > emails2) {
                            compEmail2 := 1
                        } else {
                            if (emails1 == emails2) {
                                compEmail2 := 0
                            } else {
                                compEmail2 := -1
                            }
                        }
                        res := compEmail2
                    } else {
                        res := 0
                    }
                } else {
                    res := compLastName1
                }
            } else {
                if (emails1 != 0 && emails2 != 0) {
                    var compEmail3: Int
                    if (emails1 > emails2) {
                        compEmail3 := 1
                    } else {
                        if (emails1 == emails2) {
                            compEmail3 := 0
                        } else {
                            compEmail3 := -1
                        }
                    }
                    res := compEmail3
                } else {
                    res := 0
                }
            }
    }
}


