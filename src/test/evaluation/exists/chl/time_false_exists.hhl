// P1
method compare1(ora1: Int, voltot1: Int, ora2: Int, voltot2: Int, i: Int) returns (res: Int)
requires exists _s1: State, _s2: State :: <_s1> && <_s2> && _s1[i] == 1 && _s2[i] == 2 &&
        _s1[ora1] == _s2[ora2] && _s1[voltot1] == _s2[voltot2] &&
        _s1[ora2] == _s2[ora1] && _s1[voltot2] == _s2[voltot1] &&
        _s1[ora1] == _s1[ora2] && _s1[voltot1] == _s1[voltot2]
ensures exists _s1: State, _s2: State :: <_s1> && <_s2> && _s1[i] == 1 && _s2[i] == 2 &&
        _s1[res] != (-1) * _s2[res]
{
        if (ora1 == ora2) {
            if (voltot1 > voltot2) {
                res := 1
            } else {
                res := -1
            }
        } else {
            if (ora1 > ora2) {
                res := 1 
            } else {
                res := -1
            }
        } 
}