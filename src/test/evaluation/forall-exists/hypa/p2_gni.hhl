method p2_gni() returns (h: Int, b: Int, o: Int)
ensures forall <_s1>, <_s2> :: (exists <_s3> :: _s3[h] == _s2[h] && _s3[o] == _s1[o] && _s3[b] == _s1[b])
{
    o := 0

    havoc b {hint1}

    havoc h {hint2}

    var l: Int
    havoc l {hint3}

    b := b + l

    use forall <_s1>, <_s2> :: hint1(1) && hint2(_s2[h]) && hint3(_s1[b] - 1)
}