method infBranchTS_GNI(low: Int, high: Int) returns (res: Int)
ensures forall <_s1>, <_s2> ::
    (exists <_s> :: _s[high] == _s1[high]
                                && _s[res] == _s2[res]
                                && _s[low] == _s2[low]
    )
{
    var x: Int
    if (high > 0) {
        while (x > 0) {
            var y: Int
            havoc y
            if (y > 1) {
                res := x - y
            } else {
                res := x - 1
            }
        }
    } else {
        while (x > 0) {
            res := x - 1
        }
    }
}