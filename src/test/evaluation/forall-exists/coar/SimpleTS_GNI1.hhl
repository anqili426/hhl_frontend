method simpleTS_GNI1(low: Int, high: Int) returns (res: Int)
ensures forall <_s1>, <_s2> ::
    (exists <_s> :: _s[high] == _s1[high]
                                && _s[res] == _s2[res]
                                && _s[low] == _s2[low]
    )
{
    var i: Int
    var x: Int
    havoc i
    while (i > 0) 
    invariant 
    {
        x := x + 1
        havoc i 
    }
    res := high + x    
}