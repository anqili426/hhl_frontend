method simpleTS_GNI2(low: Int, high: Int) returns (res: Int)
ensures forall <_s1>, <_s2> ::
    (exists <_s> :: _s[high] == _s1[high]
                                && _s[res] == _s2[res]
                                && _s[low] == _s2[low]
    )
{
    var x: Int
    x := high
    var i: Int
    havoc i
    while (i > 0) {
        x := x + 1
        havoc i
    }
    res := x
}